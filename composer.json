{
  "name": "johnnymcweed/luya-module-service",
  "description": "WIP! - The service module will provide you a basic system to show your services and link each over. Moreover there is an adjustable service calculator coming",
  "type": "luya-module",
  "minimum-stability": "stable",
  "keywords": ["php", "yii", "yii2", "luya", "luya-module", "module", "service"],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Schmid",
      "email": "alex.schmid@stud.unibas.ch"
    }
  ],
  "support": {
    "issues": "https://github.com/JohnnyMcWeed/luya-module-service/issues"
  },
  "require": {
    "creocoder/yii2-nested-sets": "0.9.*"
  },
  "autoload" : {
    "psr-4" : {
      "johnnymcweed\\service\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "johnnymcweed\\service\\tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}